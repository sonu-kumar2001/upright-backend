{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "max-len": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "no-await-in-loop": "off",
    "no-restricted-syntax": "off",
    "no-param-reassign": "off",
    "quotes": "off",
    "linebreak-style": "off",
    "comma-dangle": "off"
  }
}
